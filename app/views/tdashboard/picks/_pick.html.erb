<div class="bet-#{pick.id}">
  <div class="row">
    <div class="col-md-12 betslip-match">
      <div class="pull-left">
        <%= pick.bettype %>, <%= pick.match.date.strftime("%d %b %Y : %H:%M") %>
      </div>
      <div class="pull-right">
        <%= link_to "x", tdashboard_pick_path(pick.id), :method => :delete, class: "delete-bet", remote: true %>
      </div>
    </div>
  </div>
  <div class="row betslip-match">
    <div class="col-md-9">
      <%= pick.match.home_team %> vs <%= pick.match.away_team %>
    </div>
    <div class="col-md-3">
    </div>
    <div class="row betslip-match">
      <div class="col-md-8 team">
        <%= pick.team %>
      </div>
      <div class="col-md-4 odds">
        <%= number_with_precision(pick.odds, precision: 2) %>
      </div>
    </div>
  </div>
</div>
      
        
          
    